{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "debug": false,
        "modules": false,
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "useBuiltIns": "usage",
        "targets": "last 1 chrome version"
      }
    ],
    [
      "@babel/preset-react",
      {
        "useBuiltIns": true
      }
    ]
  ],
  "plugins": ["react-hot-loader/babel"]
}
